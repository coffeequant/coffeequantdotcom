<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/dojo/1.10.4/dojo/dojo.js" data-dojo-config="parseOnLoad:true, isDebug: true, defaultTestTheme:'soria'"></script>
<script type='text/javascript'>
dojo.require("dojox.widget.Roller");
dojo.require("dojox.layout.ContentPane");
dojo.require("dojox.collections.Dictionary");
</script>

<script type='text/javascript'>
var demo;
require(["dojo/_base/declare","dojo/dom","dojo/dom-style"],
function(declare, dom, domStyle){
var Demo = declare(null, {
overlayNode:null,
constructor:function(){
		this.overlayNode = dom.byId("loadingOverlay");
},
endLoading: function() {
		var overlayNode = dom.byId("loadingOverlay");
		domStyle.set(overlayNode, 'display', 'none');
},
});
demo = new Demo();
});

require(["dojo/_base/lang", "dojo/domReady!"], function (lang) {
    setTimeout(function() {
        demo.endLoading();
    }, 1000);
});

		require([
    "dijit/MenuBar",
    "dijit/PopupMenuBarItem",
    "dijit/Menu",
    "dijit/MenuItem",
    "dijit/DropDownMenu",
    "dojo/domReady!",
		"dojox/collections/Dictionary",
		"dojox/collections/ArrayList"
], function(MenuBar, PopupMenuBarItem, Menu, MenuItem, DropDownMenu){
    var coffeebar = new MenuBar({});
		var quantbar = new MenuBar({});
		var coffeemenus = new dojox.collections.Dictionary();
		var quantmenus = new dojox.collections.Dictionary();

		coffeemenus.add("Articles",new dojox.collections.ArrayList(["BiasVariance","CMSConvexity","PDEs","AutoCall Hedge Note","Numeraire","LookBacks Using FD","Correlation Stock Vol", "Bad Kelly","Outperformance","Finite Differences","Worst of Put","Risk Neutral Probability","Hedging","Barriers"]));
		coffeemenus.add("SimplePuzzles",new dojox.collections.ArrayList(["Coin Game","Quiz Answers","Brownian Puzzle","Dice Puzzle","Cart Problem","Road Path","Marriage Problem"]));
		coffeemenus.add("Videos",new dojox.collections.ArrayList(["How to Price Autocallable","Coffee Monad",]));
		quantmenus.add("Pricers",new dojox.collections.ArrayList(["Autocallable","Range Accrual","Static Hedge","Vanilla Portfolio"]));
		coffeemenus.add("Python",new dojox.collections.ArrayList(["Stochastic Volatility Models","Spectrograms","Monads In Python"]));
		quantmenus.add("Haskell",new dojox.collections.ArrayList(["Monad Example","Bored Of Programming","Sudoku Haskell"]));
		coffeemenus.add("Interview Questions",new dojox.collections.ArrayList(["Exotics","Exotic Derivatives","Vanillas","Vanillas More","Mixed","True False"]));
		quantmenus.add("Utilities",new dojox.collections.ArrayList(["CodeSnippets","Algorithms","WHOAMI"]));

		dojo.forEach(coffeemenus.getKeyList(),function(a) {
			  var tmp = new DropDownMenu({});
				dojo.forEach(coffeemenus.item(a).toArray(),function(b) { tmp.addChild(new MenuItem({label:String(b), onClick: function() {
					dijit.registry.byId("myContentPane").executeScripts="true";
					if(a=="Pricers" || a=="Interview Questions") {
						dijit.registry.byId("myContentPane").setHref("statics/"+String(b).replace(/\s/g,'')+".html");
					}
					else { dijit.registry.byId("myContentPane").setHref("statics/WriteUps/"+String(b).replace(/\s/g,'')+".html"); }
		     } })); });
				coffeebar.addChild(new PopupMenuBarItem({label:String(a), popup: tmp}));
		} );

		dojo.forEach(quantmenus.getKeyList(),function(a) {
			  var tmp = new DropDownMenu({});
				dojo.forEach(quantmenus.item(a).toArray(),function(b) { tmp.addChild(new MenuItem({label:String(b), onClick: function() {
					dijit.registry.byId("myContentPane").executeScripts="true";
					if(a=="Pricers" || a=="Interview Questions") {
						dijit.registry.byId("myContentPane").setHref("statics/"+String(b).replace(/\s/g,'')+".html");
					}
					else { dijit.registry.byId("myContentPane").setHref("statics/WriteUps/"+String(b).replace(/\s/g,'')+".html"); }
		     } })); });
				quantbar.addChild(new PopupMenuBarItem({label:String(a), popup: tmp}));
		} );

		coffeebar.placeAt("coffee");
		quantbar.placeAt("quant");
		coffeebar.startup();
		quantbar.startup();

});

		</script>
