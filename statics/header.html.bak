<!DOCTYPE HTML> 
<html lang="en"> 
	<head> 
	<style type="text/css">
                @import "http://ajax.googleapis.com/ajax/libs/dojo/1.6dojox/grid/resources/tundraGrid.css";
            @import "dojor/dijit/themes/tundra/tundra.css";

            @import "dojor/dojox/grid/resources/Grid.css";
            @import "dojor/dijit/themes/claro/claro.css";
            @import "dojor/dojox/widget/FisheyeList/FisheyeList.css";
            @import "dojor/dojox/widget/Toaster/Toaster.css";

	    @import "customstyles/CustomFishEye.css";
		
            .dojoxGrid table { margin: 0; } html, body { width: 100%; height: 100%;
            margin: 0; }
body {
  background-image: url("images/background.png");
}

	</style>
        	<meta charset="utf-8"> 
		<title>CoffeeQuant</title> 
 
		<!-- load dojo and provide config via data attribute --> 
		<script src="dojo.xd.js"
				data-dojo-config="isDebug: true,parseOnLoad: true"> 
		</script> 
	<script> 
        dojo.require("dijit.form.ComboBox");
        dojo.require("dojox.widget.FisheyeList");					
	dojo.require("dojox.widget.Roller");	
	dojo.require("dojox.form.RangeSlider");
	dojo.require("dijit.form.Button");
	dojo.require("dojox.grid.DataGrid");
        dojo.require("dojo.data.ItemFileWriteStore");
	dojo.require("dijit.form.NumberTextBox");
	dojo.require("dijit.layout.ContentPane");
    dojo.require("dojox.widget.Toaster");
    
    dojo.require("dojox.timing");

function check()
{
t = new dojox.timing.Timer(30000);
t.onTick = function(){
 surpriseMe();
}
t.onStart = function(){
 console.info("Starting timer");
}
t.start();

}
	       

        dojo.require("dijit.ProgressBar");
        

        
        function clickeye(pageName)
        {
                    var myCp= parent.dojo.byId("treecontent");
                    myCp.src=pageName;
            parent.dojo.byId('treecontent').height = 1800;

                    
//                    parent.window.refresh();
                      parent.window.scrollTo(0,300);
                        

        }
        
            function surpriseMe(){
   dijit.byId('first_toaster').setContent('Pricers , Graphs & Quizzes !', 'fatal');
   dijit.byId('first_toaster').show();
   
   dijit.byId('first_toaster').setContent('send feedback - animeshsaxena@coffeequant.com', 'fatal');
   dijit.byId('first_toaster').show();

}

dojo.addOnLoad(check);


	</script> 
	</head> 
	<body class="tundra"> 

<div data-dojo-type="dojox.widget.Toaster"
 data-dojo-props="positionDirection:'br-right'"
     id="second_toaster">
</div>


<div class="outerbar">

<div dojoType="dojox.widget.FisheyeList"
		itemWidth="150" itemHeight="120"
		itemMaxWidth="250" itemMaxHeight="150"
		orientation="horizontal"
		effectUnits="2"
		itemPadding="15"
		attachEdge="top"
		labelEdge="bottom"
		id="fisheye1"
	>
    
    <div dojoType="dojox.widget.FisheyeListItem"
		      onclick="clickeye('/statics/quiz.html');"
		iconSrc="images/quiz.png">
	</div>

	<div dojoType="dojox.widget.FisheyeListItem"
		onclick="clickeye('http://coffeequant4.appspot.com/statics/rangeaccrualpart.html');"
		iconSrc="images/rangeaccrual.png">
	</div>

	<div dojoType="dojox.widget.FisheyeListItem" align="center"
		onclick="clickeye('http://coffeequant3.appspot.com/statics/portfolioanalyzerpart.html');"
		iconSrc="images/portfolio.png">
	</div>


	<div dojoType="dojox.widget.FisheyeListItem"
		onclick="clickeye('http://coffeequant2.appspot.com/statics/advancedtoolspart.html');"
		iconSrc="images/scenario.png">
	</div>
	
	<div dojoType="dojox.widget.FisheyeListItem"
		onclick="clickeye('http://coffeequant6.appspot.com/statics/basictoolspart.html');"
		iconSrc="images/barriers.png">
	</div>
	
	

	<div dojoType="dojox.widget.FisheyeListItem"
		onclick="clickeye('http://coffeequant1.appspot.com/statics/autocallablepart.html');"
		iconSrc="images/autocall.png">
	</div>


	<div dojoType="dojox.widget.FisheyeListItem"
		onclick="clickeye('http://coffeequant5.appspot.com/statics/graphtest.html');"
		iconSrc="images/vanilla.png">
	</div>
    



</div>
</div>

<div data-dojo-type="dojox.widget.Toaster" data-dojo-props="positionDirection:'br-left'"
     id="first_toaster">
</div>

</body> 
</html> 
